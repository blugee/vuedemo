extends ../appLayout

block nav
  .navbar.navbar-default.navbar-fixed-top#nav(v-cloak)
    .container(style='width: 100%; margin-right: 0px; margin-left: 0px;')
      .navbar-header
        button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='#', @click.prevent='hamburgerClicked', v-show='sharedState.currentUser')
          i.fa.fa-bars
      .collapse.navbar-collapse
        //- Are not signed in
        ul.nav.navbar-nav.navbar-left.unsigned-in(v-show='sharedState.currentUser.anon')
          li
            a.btn.btn-default(href='#', @click='saveYourDesign') {{ sharedState.saveyourdesignLabel }}

        //- Are signed in
        ul.nav.navbar-nav.navbar-left.signed-in-nav

          li.file-name(v-show='!sharedState.currentUser.anon')
            input#file-name.form-control(placeholder='Project Name', @blur='setTitle', @keyup.enter='setTitle', v-model='sharedState.currentDesign.name', data-toggle='tooltip', data-placement='bottom', :data-original-title='sharedState.renameProjectTooltip')

          li.project-action(
                v-show='!sharedState.currentUser.anon',
                class="delete active",
                data-toggle='tooltip',
                data-placement='bottom',
                :data-original-title='sharedState.deleteProjectTooltip'
                @click="showConfirmRemoveProject"
            )
            i.fa.fa-trash-o

          li.revision-name(v-show='!sharedState.currentUser.anon')
            .btn-group
              input#revision-name.form-control(placeholder='Revision', @blur='setRevisionTitle', @keyup.enter='setRevisionTitle' v-model='sharedState.currentDesign.currentRev.name', :disabled='!sharedState.currentDesign', data-toggle='tooltip', data-placement='bottom', :data-original-title='sharedState.renameDesignTooltip')
              button.btn.btn-default.dropdown-toggle(v-if='sharedState.currentDesign.softLocked', @click='showSoftLockMessage(sharedState.designSoftLockedMessageNewDesignLabel)', type='button', :disabled='!sharedState.currentDesign')
                span.caret
              button.btn.btn-default.dropdown-toggle(v-else, type='button', data-toggle='dropdown', data-hover-toggle='tooltip', data-placement='bottom', data-original-title='Select Design', aria-haspopup='true', aria-expanded='false', :disabled='!sharedState.currentDesign')
                span.caret
                span.sr-only Toggle Dropdown
              ul.dropdown-menu
                li
                  a(href:='#{{sharedState.duplicateDesignlabel}}', @click.prevent='createRevision', data-toggle='tooltip', data-placement='right', data-container='body', :data-original-title='sharedState.createCopyofthisdesignlabel') {{sharedState.duplicateDesignlabel}}
                li.divider

                li(v-for='rev in sharedState.currentDesign.revisions')
                  a(href:='#{{rev.name}}', @click.prevent='setRevision(rev._id)')
                    i.fa.fa-check(:class="{'active':(rev._id === sharedState.currentDesign.currentRev._id)}")
                    | {{rev.name}}

          li.revision-action(
                v-show='!sharedState.currentUser.anon && (sharedState.currentDesign && sharedState.currentDesign.revisions && sharedState.currentDesign.revisions.length)',
                class="delete active",
                data-toggle='tooltip',
                data-placement='bottom',
                :data-original-title='sharedState.deleteDesignTooltip',
                @click="showConfirmDeleteDesign"
            )
            i.fa.fa-trash-o

          li.save-status(v-show='sharedState.currentUser && sharedState.currentDesign._id')
            | {{sharedState.currentSaveStatus}}
          li.file-status(v-show='sharedState.currentDesign._id', data-cypress="file-status" )
            span {{ sharedState.fileidLabel }}
              span(style='margin-left:2px', data-cypress="file-status-span-fileID") {{sharedState.currentDesign.currentRev.fileID}}
            input(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isRep || sharedState.currentUser.isExporter', type='text', placeholder='Find by ID', v-model='privateState.adminFile', @keyup.13='adminFindFile')
            label(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate')
              input(type='checkbox', v-model='sharedState.adminOverride')
              | &nbsp;Override Save
          li.center-logo
            if store
              a(href='#{store.logoURL ? store.logoURL : "https://www.mycasebuilder.com"}')
                if store.hasCustomLogo
                  img(src='#{store.logoImg}', style='height: auto; width: #{store.logoImgWidth || "60px"}; margin-left: -100%;')
                else
                  img(src='/imgs/MCB_Logo_RGB.svg', style='height: 58px; width: auto; margin-left: -100%;')
            else
              a(href='https://www.mycasebuilder.com')
                img(src='/imgs/MCB_Logo_RGB.svg', style='height: 58px; width: auto; margin-left: -100%;')

        ul.nav.navbar-nav.navbar-right.unsigned-in(v-if='sharedState.currentUser.anon && !sharedState.suggestCaseMode')
          li
            a#login-modal-open-btn(href='#', @click='loginClicked', data-cypress='user-login') {{ sharedState.loginLabel }}
          li
            a(href='#', @click='registerClicked', data-cypress='account-register') {{ sharedState.createaccountLabel }}
        ul.nav.navbar-nav.navbar-right.unsigned-in(v-else)  
          li(v-if='showNewShoppingCart && sharedState.suggestCaseMode && sharedState.currentUser.anon')
            a#order.btn.btn-default.mt-2.mx-2(href='#', @click='suggestCase')
              span {{sharedState.suggestCaseModelabel}}
        ul.nav.navbar-nav.navbar-right.user-status(v-if='!sharedState.currentUser.anon')
          li(style='display:none')
            a#downloadDXFZip.z-btn.btn.btn-default(:href='sharedState.dxfData', :download='sharedState.currentDesign.currentRev.fileID + ".zip"', v-el:dxf-download)
          li(style='display:none')
            a#downloadPDF.z-btn.btn.btn-default(:href='sharedState.pdfData', :download='sharedState.currentDesign.currentRev.fileID + ".pdf"', v-el:pdf-download)
          li(v-if='sharedState.currentUser.isAdmin && sharedState.currentDesign.currentRev.publicDesign && sharedState.currentDesign.currentRev.publicDesign.isCandidate')
            a.z-btn.btn.btn-default#public-design-status-btn(href='#', @click='openPublicDesignStatus()')
              | Public Design Status
          li(v-if='(sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isExporter)')
            a.z-btn.btn.btn-default#admin-exports-btn(href='#', @click='exportPdf()')
              | Exports
          li.dropdown.unimplemented
            a.btn.btn-default.dropdown-toggle(href='#', data-toggle='dropdown', aria-expanded='false')
              | Share
              i.caret
            ul.dropdown-menu
              li
                div.shareable-area
                  h4
                    | Sharing
                  p
                    | Sharing allows you to design and chat with others in real time.
                  div.checkbox
                    label(@click.stop)
                      input(type='checkbox', @change.stop='updateSharable', v-model='sharedState.currentDesign.shareable')
                      | Shareable Design
                  div.shareable-link-area(v-show='sharedState.currentDesign.shareable')
                    label(for='shareable-link')
                      | Shareable Link
                      a(href='#', @click.stop='copyText') copy
                    input#shareable-link.form-control(v-model='shareableLink', readonly='readonly', @click.stop='selectShareableLink')
                    p Give this link to someone you want to collaborate with.

          li.dropdown(style="height:48px")
            a.dropdown-toggle(href='#', data-toggle='dropdown', aria-expanded='false', data-cypress='app-user-menu')
              img(:src='sharedState.currentUser.gravatarURL')
              i.caret
            ul.dropdown-menu
              li
                a(href='#my-account', onclick="javascript:mcb.ui.modals.accountProfile.vue.toggle(); return false;", data-cypress='my-account') {{ sharedState.myAccountLabel }}
              li.divider
              li
                a(href='app/orders', data-cypress='my-orders') {{sharedState.myOrders}}
              li.divider
              li
                a(href='#change-password', onclick="javascript:mcb.ui.modals.changePassword.vue.toggle(); return false;", data-cypress='change-password') {{ sharedState.changePasswordLabel }}
              li.divider
              li
                a(href='#', @click='logout', data-cypress='logout') {{ sharedState.logoutLabel }}
          li(v-if='showNewShoppingCart && sharedState.cartItems.length')
            a#open-cart(href='#', @click='openCart()')
              i.fa.fa-shopping-cart
              span {{sharedState.openCartLabel}} ({{ sharedState.cartItems.length }})
          li(v-if='showNewShoppingCart && !sharedState.suggestCaseMode')
            a#order.btn.btn-default.mt-2.mx-2(href='#', @click='order')
              span {{ sharedState.addToCartLabel }}
          li(v-if='showNewShoppingCart && sharedState.suggestCaseMode')
            a#order.btn.btn-default.mt-2.mx-2(href='#', @click='suggestCase')
              span {{sharedState.suggestCaseModelabel}}
          li(v-if='!showNewShoppingCart')
            a#order(href='#', @click='order')
              i.fa.fa-shopping-cart
              span {{ sharedState.orderLabel }}

block content
  #properties-panel
  #left-nav-menu(v-cloak)
    ul.nav-buttons
      // hide the shape library only when the store is Japan (JPN)
      li
        a(href='#', @click='shapeLibraryClicked')
          i.fa.fa-puzzle-piece
          span {{ sharedState.libraryLabel }}
      li
        a(href='#', @click='photoClicked')
          i.fa.fa-camera-retro
          span {{ sharedState.photoLabel }}
      li
        a(href='#', @click="toolSelected('rect')")
          i.fa.fa-square-o
          span {{ sharedState.rectLabel }}
      li
        a(href='#', @click="toolSelected('circle')")
          i.fa.fa-circle-o
          span {{ sharedState.circleLabel }}
      li
        a(href='#', @click="drawClicked")
          i.fa.fa-pencil
          span {{ sharedState.drawLabel }}
      li.divider
      li.unimplemented
        a(href='#', @click="toolSelected('case')")
          i.fa.fa-briefcase
          span Layers
      li
        a(href='#', @click="toolSelected('settings')")
          i.fa.fa-cog
          span {{ sharedState.settingslabel }}

      li(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isRep || sharedState.currentUser.isLibrarian')
        a(href='#', @click="toolSelected('admin')")
          i.fa.fa-lock
          span {{ sharedState.adminLabel }}

  #sub-nav-menu(:class="{'active':sharedState.subMenuActive}")
    a.close-sub-nav(href='#', @click='closeSubClicked')
      i.fa.fa-times
    ul#rect-nav.sub-nav-buttons
      li
        h4.heading
          | {{ sharedState.addrectangleLabel }}
      li
        .checkbox
          input#rounded-rect-toggle(type='checkbox', v-model='privateState.isRoundedRect')
          label(for='rounded-rect-toggle') {{ sharedState.roundedRectangleLabel }}
      li
        .form-group(:class='{"has-error":privateState.errors.length.hasError, "has-warning":privateState.errors.length.hasWarning}')
          label(for='rect-length-input') {{sharedState.lengthLabel}} ({{unitI18N}})
          input#rect-length-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.rect.length',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='length',
            v-el:rect-length,
            v-limit-numeric
          )
      li
        .form-group(:class='{"has-error":privateState.errors.width.hasError, "has-warning":privateState.errors.width.hasWarning}')
          label(for='rect-width-input') {{ sharedState.widthLabel }} ({{unitI18N}})
          input#rect-width-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.rect.width',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='width',
            v-el:rect-width,
            v-limit-numeric
          )
      li
        .form-group(:class='{"has-error":privateState.errors.depth.hasError}')
          label(for='rect-depth-input') {{ sharedState.depthLabel }} ({{unitI18N}})
          input#rect-depth-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.rect.depth',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='depth',
            v-el:rect-depth,
            v-limit-numeric
          )
      li
        .form-group(v-if="privateState.isRoundedRect", :class='{"has-error":privateState.errors.corner.hasError, "has-warning":privateState.errors.fingerNotches.hasWarning, "disabled": disableCornerRound}')
          label(for='rect-corner-input') {{ sharedState.cornerRadiuslabel }} ({{unitI18N}})
          i.fa.fa-info-circle.fa-pull-right(data-toggle='tooltip', data-placement='right', data-container='body', data-html='true', :data-original-title='translateTooltip("","radius-corner.jpg")')
          input#rect-corner-input.form-control(
            type='text',
            placeholder="0",
            :disabled='disableCornerRound',
            v-model='privateState.rect.corner',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='corner',
            v-el:rect-corner,
            v-limit-numeric
          )
      li
        .form-group(:class='{"has-error":privateState.errors.fingerNotches.hasError}')
          label(for='fingerNotchRect') {{sharedState.fingerNotchesLabel}}
          i.fa.fa-info-circle.fa-pull-right(data-toggle='tooltip', data-placement='right', data-container='body', data-html='true', :data-original-title='sharedState.notchBuilderToolTip')
          select.form-control#fingerNotchRect(v-model='fingerNotches', :class='{"has-error":privateState.errors.fingerNotches.hasError}', @change='checkErrors')
            option(value='', disabled, selected) {{ sharedState.setFingernotches }}
            option(value='top') {{ sharedState.topAndbottom }}
            option(value='left') {{ sharedState.leftAndright }}
            option(value='builder') {{ sharedState.notchBuilderLabel }}
            option(value='none') {{ sharedState.noneLabel }}
          span.help-block(v-show='privateState.errors.fingerNotches.hasError')
            | {{ privateState.errors.fingerNotches.message }}
      li
        a.btn.btn-primary.btn-sm(href='#', @click="addShape('rect')")
          | {{ sharedState.addrectangleLabel }}
      li
        img(src='/imgs/rectangle_menu.png')

    ul#circle-nav.sub-nav-buttons
      li
        h4.heading
          | {{ sharedState.addCircle }}
      li
        label.circle-selectors(for='circle-type-circle') {{ sharedState.circleLabel }}
          input#circle-type-circle(type='radio', v-model='privateState.circleType', name='circle-type', autocomplete='off', checked='', value='circle', @change='circleTypeChange')
        label.circle-selectors(for='circle-type-oval') {{ sharedState.ovalLabel }}
          input#circle-type-oval(type='radio', v-model='privateState.circleType', name='circle-type', autocomplete='off', value='oval', @change='circleTypeChange')
      li(v-show='privateState.circleType === "circle"')
        .form-group(:class='{"has-error":privateState.errors.diameter.hasError, "has-warning":privateState.errors.diameter.hasWarning}')
          label(for='circle-diameter-input') {{sharedState.diameterLabel}}  ({{unitI18N}})
          input#circle-diameter-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.circle.diameter',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='diameter',
            v-el:circle-diameter,
            v-limit-numeric
          )
      li(v-show='privateState.circleType === "oval"')
        .form-group(:class='{"has-error":privateState.errors.length.hasError, "has-warning":privateState.errors.length.hasWarning}')
          label(for='circle-length-input') {{sharedState.lengthLabel}} ({{unitI18N}})
          input#circle-length-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.circle.length',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='length',
            v-el:circle-length,
            v-limit-numeric
          )
      li(v-show='privateState.circleType === "oval"')
        .form-group(:class='{"has-error":privateState.errors.width.hasError, "has-warning":privateState.errors.width.hasWarning}')
          label(for='circle-width-input') {{sharedState.widthLabel}} ({{unitI18N}})
          input#circle-width-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.circle.width',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='width',
            v-el:circle-width,
            v-limit-numeric
          )
      li
        .form-group(:class='{"has-error":privateState.errors.depth.hasError}')
          label(for='circle-depth-input') {{sharedState.depthLabel}} ({{unitI18N}})
          input#circle-depth-input.form-control(
            type='text',
            placeholder="0",
            v-model='privateState.circle.depth',
            @blur.capture='textInputBlur($event, true)',
            @keyup.enter.stop='textInputBlur',
            @keyup='textInputBlur($event, true)',
            name='depth',
            v-el:circle-depth,
            v-limit-numeric
          )
      li
        .form-group(:class='{"has-error":privateState.errors.fingerNotches.hasError}')
          label(for='fingerNotchCircle') {{sharedState.fingerNotchesLabel}}
          i.fa.fa-info-circle.fa-pull-right(data-toggle='tooltip', data-placement='right', data-container='body', data-html='true', :data-original-title='sharedState.notchBuilderToolTip')
          select.form-control#fingerNotchCircle(v-model='fingerNotches', :class='{"has-error":privateState.errors.fingerNotches.hasError}', @change='checkErrors')
            option(value='', disabled, selected) {{sharedState.setFingernotches}}
            option(value='top') {{sharedState.topAndbottom}}
            option(value='left') {{sharedState.leftAndright}}
            option(value='builder') {{ sharedState.notchBuilderLabel }}
            option(value='none') {{ sharedState.noneLabel }}
          span.help-block(v-show='privateState.errors.fingerNotches.hasError')
            | {{ privateState.errors.fingerNotches.message }}
      li
        a.btn.btn-primary.btn-sm(href='#', @click="addShape('circle')")
          | {{sharedState.addCircle}}
      li
        img(src='/imgs/circle_menu.png')

    ul#settings-nav.sub-nav-buttons
      li
        p
          | {{sharedState.settingslabel}}
      ul.list-unstyled(data-menu='grid')
        li
          .checkbox
            label.checkbox(for='show-grid-checkbox')
              input(id='show-grid-checkbox' type='checkbox', v-model='sharedState.appSettings.showGrid', @change='toggleGrid(sharedState.appSettings.showGrid)')
              | {{sharedState.showGridlabel}}
        li
          .checkbox
            label.checkbox(for='snap-grid-checkbox')
              input(id='snap-grid-checkbox' type='checkbox', v-model='sharedState.appSettings.snapToGrid', @change='toggleSnap(sharedState.appSettings.snapToGrid)')
              | {{sharedState.snapTogridlabel}}
        li
          .checkbox
            label.checkbox(for='depth-grid-checkbox')
              input(id='depth-grid-checkbox' type='checkbox', v-model='sharedState.appSettings.showDepthLabels')
              | {{sharedState.showDepthlabelsLabel}}
        li
          .checkbox
            label.checkbox(for='auto-snap-checkbox', :style="!canAutoSnap ? 'cursor:default' : ''")
              input(id='auto-snap-checkbox' type='checkbox', :disabled='!canAutoSnap', v-model='sharedState.appSettings.autoSnapShapes')
              | {{sharedState.autoSnapLabel}}
        li.divider.mb-2(role='separator')
        li
          .form-group(:class='{"has-error":privateState.settings.gridStepError}')
            label(for='grid-size-input') {{sharedState.gridSizelabel}} ({{unitI18N}})
            input#grid-size-input.form-control(type='text', v-model='gridStep_display', v-limit-numeric)
            span.help-block(v-show='privateState.settings.gridStepError')
              | {{ privateState.settings.gridStepMessage }}
        li
          .form-group(:class='{"has-error":sharedState.nudgeSpacing.hasError}')
            label(for='nudge-spacing-input') {{sharedState.nudgeSpacinglabel}} ({{unitI18N}})
            i.fa.fa-info-circle.fa-pull-right(data-toggle='tooltip', data-placement='right', data-container='body', data-html='true', :data-original-title='sharedState.nudgeTooltip')
            input.form-control(id='nudge-spacing-input', type='text', v-model='nudgeSpacing_display', v-limit-numeric)
            span.help-block(v-show='sharedState.nudgeSpacing.hasError')
              | {{ sharedState.nudgeSpacing.message }}

      li.divider(role='separator')

      li
        p
          | {{sharedState.unitsLabel}}
      li
        .radio
          label
            input#inches-setting(type='radio', name='units', value='inches', @click='toggleUnits("inches")', :checked='sharedState.currentUnit === "inches"')
            | {{sharedState.inchesLabel}}
        .radio
          label
            input#mm-setting(type='radio', name='units', value='mm', @click='toggleUnits("mm")', :checked='sharedState.currentUnit === "mm"')
            | {{sharedState.mmLabel}}

      li.divider(role='separator')

      li
        p
          | Select Language
        select(id='language', v-model='sharedState.appSettings.language')
          option(value='en') English
          option(value='nl') Dutch
          option(value='fr') French
          option(value='de') German
          option(value='it') Italian
          option(value='ja') Japanese
          option(value='es') Spanish

      li.divider.mb-2.mt-3(role='separator', v-show="!sharedState.currentUser.anon")
      li
        a.btn.btn-primary.btn-block.mt-3( @click='closeAppSettings()')
          | {{sharedState.saveSettingsLabel}}
      li(v-show="!sharedState.currentUser.anon")
        a.btn.btn-primary.btn-block.mt-3( @click='saveAppSettings()')
          | {{sharedState.rememberSettingsLabel}}
    ul#admin-nav.sub-nav-buttons
      li
        ul.list-unstyled
          li.active(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isLibrarian')
            a(href='#', @click='privateState.admin.currentMenu="shapelibrary"')
              | Shape Library
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="incaseshape"')
              | In Case Shapes
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="brandscategories"')
              | Brands and Categories
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="userfiles"')
              | Files
          li(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isRep')
            a(href='#', @click='privateState.admin.currentMenu="listofusers"')
              | Users
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="templates"')
              | Templates
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="import"')
              | Import
          li(v-if='sharedState.currentUser.isAdmin')
            a(href='#', @click='privateState.admin.currentMenu="previewLink"')
              | Preview Link

      li.admin-menu(v-show='privateState.admin.currentMenu==="templates"', v-if='sharedState.currentUser.isAdmin')
        p(v-if='sharedState.currentUser.isAdmin')
          a.z-btn.btn.btn-default(href='#', title='Make this design not a template', v-if='sharedState.currentDesign.isTemplate', @click='adminCreateTemplate(sharedState.currentDesign, false)')
            | Remove template
          a.z-btn.btn.btn-default(href='#', title='Make this design a template', v-if='!sharedState.currentDesign.isTemplate', @click='adminCreateTemplate(sharedState.currentDesign, true)')
            | Make template
        p(v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isRep || sharedState.currentUser.isExporter')
          a.z-btn.btn.btn-default(href='#', title='Make a local copy of this design', v-if='sharedState.currentDesign.isTemplate', @click='makeLocalCopyFromTemplate')
            | Make local copy

      li.admin-menu(v-show='privateState.admin.currentMenu==="userfiles"', v-if='sharedState.currentUser.isAdmin')
        p
          | user files go here.

      li.admin-menu(v-show='privateState.admin.currentMenu==="listofusers"', v-if='sharedState.currentUser.isAdmin || sharedState.currentUser.isAffiliate || sharedState.currentUser.isRep')
        label
          | Search by email
          input(type='text', placeholder='Search by email', v-model='privateState.admin.emailSearch', @keyup.13='adminSearchForUser')
        p
          ul.list-unstyled
