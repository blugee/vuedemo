    "build": "DEBUG=off ./node_modules/gulp/bin/gulp.js",
    "watch": "DEBUG=off ./node_modules/gulp/bin/gulp.js watch",
    "production": "DEBUG=off ./node_modules/gulp/bin/gulp.js production",
    "serve": "./node_modules/nodemon/bin/nodemon.js ./bin/www",
    "serve-debug": "./node_modules/nodemon/bin/nodemon.js --inspect=127.0.0.1:3029 ./bin/www",
    "dev": "npm run build; npm run watch & npm run serve-debug",
    "test": "npm run mocha:run && npm run cypress:run",
    "mocha:run": "./node_modules/mocha/bin/mocha --reporter spec --timeout 5000",
    "cypress:run": "./node_modules/cypress/bin/cypress run --project test",
